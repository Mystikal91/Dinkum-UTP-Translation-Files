<resources>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_000">The name is &lt;myName&gt;.</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_001">&lt;shy&gt;I'm something of a trader myself.</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_002">&lt;no&gt;&lt;NPCName&gt;? Never heard of him.</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_003">But you know what I have heard of?</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_004">&lt;pumped&gt;Great deals.</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_005">So if you're in the mood for some of those, I'm your guy.</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_006">What was your name again?</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_007">&lt;PlayerName&gt;, I like you. I like you a lot.</string>
  <!-- Talking about NPC: John -->
  <string name="Npc11/Introduction_011_Intro_008">&lt;yes&gt;Let's do some deals. Lets trade some wheels.</string>
  <!-- Talking about NPC: Fletch -->
  <string name="Npc11/IslandDayGreeting_AfterNoon_011_Intro_000">Happy &lt;IslandDay&gt;!</string>
  <string name="Npc11/IslandDayGreeting_Morning_011_Intro_000">Happy &lt;IslandDay&gt;!</string>
