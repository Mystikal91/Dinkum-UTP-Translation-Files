  <string name="MELVIN/BackAndForthConvo_nightFallbacks_1_Line_1">Goodnight!</string>
  <string name="MELVIN/BackAndForthConvo_nightFallbacks_2_Line_0">Nice tonight.</string>
  <string name="MELVIN/BackAndForthConvo_nightFallbacks_2_Line_1">Yeah, it is nice.</string>
  <string name="MELVIN/BackAndForthConvo_nightFallbacks_2_Line_2">Really nice.</string>
  <string name="MELVIN/BackAndForthConvo_rainingFallbacks_0_Line_0">Rain beats the snow.</string>
  <string name="MELVIN/BackAndForthConvo_rainingFallbacks_0_Line_1">Too right.</string>
  <string name="MELVIN/BackAndForthConvo_rainingFallbacks_1_Line_0">Rainin’ again.</string>
  <string name="MELVIN/BackAndForthConvo_rainingFallbacks_1_Line_1">Yeah, I can see that.</string>
  <string name="MELVIN/BackAndForthConvo_rainingFallbacks_2_Line_0">My knees are playing up again.</string>
  <string name="MELVIN/BackAndForthConvo_rainingFallbacks_2_Line_1">Why is that?</string>
