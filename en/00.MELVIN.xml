  <string name="MELVIN/BugComp_Greeting 000_Response_003_000">Righto.</string>
  <string name="MELVIN/Cold_Greeting 0_Intro_000">Bloody freezin’, huh?</string>
  <string name="MELVIN/Cold_Greeting 0_Intro_001">Need somethin’?</string>
  <string name="MELVIN/Cold_Greeting 0_Response_003_000">Keep it up, young &lt;PlayerName&gt;.</string>
  <string name="MELVIN/Cold_Greeting 1_Intro_000">Hurts me bones, this cold.</string>
  <string name="MELVIN/Cold_Greeting 1_Response_003_000">Keep yer bones warm, alright?</string>
  <string name="MELVIN/Cold_Greeting 2_Intro_000">Bloody freezing today…</string>
  <string name="MELVIN/Cold_Greeting 2_Response_003_000">Keep yer knees warm, okay?</string>
