  <string name="MELVIN/BackAndForthConvo_morningFallbacks_2_Line_0">Nice mornin’</string>
  <string name="MELVIN/BackAndForthConvo_morningFallbacks_2_Line_1">It is isn’t it?</string>
  <string name="MELVIN/BackAndForthConvo_dayFallbacks_0_Line_0">Nice day.</string>
  <string name="MELVIN/BackAndForthConvo_dayFallbacks_0_Line_1">Yeah, it is.</string>
  <string name="MELVIN/BackAndForthConvo_dayFallbacks_1_Line_0">How are ya?</string>
