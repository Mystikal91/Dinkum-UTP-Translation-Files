  <string name="NED/Ned_Salesman_Intro_000">Ah, &lt;PlayerName&gt;!</string>
  <string name="NED/Ned_Salesman_Intro_001">Fancy bumping into you out here.</string>
  <string name="NED/Ned_Salesman_Intro_002">Do you need something?</string>
  <string name="NED/Ned_Salesman_Option_000">What can you make me?</string>
