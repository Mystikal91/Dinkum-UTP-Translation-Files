<resources>
  <!-- Talking about Animal Type: Bush Devil -->
  <string name="NED/CompleteBirthday_000_Intro_000">&lt;haha&gt;You sneaky &lt;animalName&gt;.</string>
  <!-- Talking about Animal Type: Bush Devil -->
  <string name="NED/CompleteBirthday_000_Intro_001">&lt;shy&gt;You really surprised me with this &lt;birthday&gt; gift, &lt;PlayerName&gt;.</string>
  <!-- Talking about Animal Type: Bush Devil -->
  <string name="NED/CompleteBirthday_000_Intro_002">Thanks a bunch, mate.</string>
  <string name="NED/CompleteRequest_000_Intro_000">&lt;shocked&gt;Oh wow!</string>
  <string name="NED/CompleteRequest_000_Intro_001">&lt;no&gt;Can’t believe how helpful you are!</string>
  <string name="NED/CompleteRequest_000_Intro_002">&lt;yes&gt;Take this, you’ve earned it!</string>
  <string name="NED/Introduction_005_Intro_000">Oh, G'day.</string>
  <string name="NED/Introduction_005_Intro_001">The name is &lt;myName&gt;.</string>
  <string name="NED/Introduction_005_Intro_002">And you are?</string>
  <string name="NED/Introduction_005_Intro_003">&lt;thinking&gt;&lt;PlayerName&gt;, huh?</string>
  <string name="NED/Introduction_005_Intro_004">&lt;yes&gt;Nice to meet you &lt;PlayerName&gt;.</string>
  <string name="NED/Introduction_005_Intro_005">I've been visiting &lt;IslandName&gt; for a while now.</string>
  <string name="NED/Introduction_005_Intro_006">You must be one of the people I heard about getting settled here.</string>
  <string name="NED/Introduction_005_Intro_007">&lt;no&gt;Move here too? Nah, not old &lt;myName&gt;.</string>
  <string name="NED/Introduction_005_Intro_008">I don't like being held down, get me?</string>
  <string name="NED/Introduction_005_Intro_009">&lt;proud&gt;I like going new places and hunting.</string>
  <string name="NED/Introduction_005_Intro_010">I can make some handy hunting gear if you want, just ask me.</string>
  <string name="NED/Introduction_005_Intro_011">I'll be visiting &lt;IslandName&gt; every now and again.</string>
  <string name="NED/Introduction_005_Intro_012">I hope we bump into each other in the future!</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/IslandDayGreeting_AfterNoon_005_Intro_000">Happy &lt;IslandDay&gt;!</string>
  <string name="NED/IslandDayGreeting_Morning_005_Intro_000">Happy &lt;IslandDay&gt;!</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/IslandDayGreeting_Night_005_Intro_000">Happy &lt;IslandDay&gt;!</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/IslandDayGreeting_Noon_005_Intro_000">Happy &lt;IslandDay&gt;!</string>
  <string name="NED/Ned_Salesman_Trapper_Intro_000">Ah, &lt;PlayerName&gt;!</string>
  <string name="NED/Ned_Salesman_Trapper_Intro_001">Always nice bumping into you.</string>
  <string name="NED/Ned_Salesman_Trapper_Intro_002">What can I do you for?</string>
  <string name="NED/Ned_Salesman_Trapper_Option_000">Want to buy anything?</string>
  <string name="NED/Ned_Salesman_Trapper_Option_001">Can you make me something?</string>
  <string name="NED/Ned_Salesman_Trapper_Option_002">I want to chat</string>
  <string name="NED/Ned_Salesman_Trapper_Option_003">Nevermind</string>
  <string name="NED/Ned_Salesman_Trapper_Response_000_000">Yeah, mate. I'll buy anything you've hunted at a premium.</string>
  <string name="NED/Ned_Salesman_Trapper_Response_001_000">Let's see what we can do for you.</string>
  <string name="NED/Ned_Salesman_Trapper_Response_003_000">&lt;wave&gt;Good hunting, mate.</string>
  <string name="NED/NothingToGiveTrapper_Intro_000">&lt;sigh&gt;Oh, what? You've got nothing for me?</string>
  <string name="NED/NothingToGiveTrapper_Intro_001">&lt;no&gt;Righto, mate.</string>
  <string name="NED/RandomConvo 0_Intro_000">Stay alert, &lt;PlayerName&gt;.</string>
  <string name="NED/RandomConvo 0_Intro_001">&lt;yes&gt;Threats can emerge from anywhere out here.</string>
  <string name="NED/RandomConvo 10_Intro_000">Maintaining your equipment is not only Mission-Critical, it’s also the best way to spend a Sundy arvo.</string>
  <string name="NED/RandomConvo 10_Intro_001">&lt;yes&gt;Fact.</string>
  <string name="NED/RandomConvo 11_Intro_000">I reckon everyone back in &lt;SouthCity&gt; needs to come here for a night or two.</string>
  <string name="NED/RandomConvo 11_Intro_001">When you're on &lt;IslandName&gt; you feel alive!</string>
  <string name="NED/RandomConvo 12_Intro_000">I've been coming to &lt;IslandName&gt; for years now.</string>
  <string name="NED/RandomConvo 12_Intro_001">It's nice to see some friendly faces when I come visit now.</string>
  <string name="NED/RandomConvo 13_Intro_000">&lt;no&gt;You should never doubt yourself &lt;PlayerName&gt;.</string>
  <string name="NED/RandomConvo 13_Intro_001">&lt;no&gt;You’re just not good enough for that yet.</string>
  <string name="NED/RandomConvo 14_Intro_000">&lt;proud&gt;The key to victory is always winning your battles!</string>
  <string name="NED/RandomConvo 15_Intro_000">The birds around here are hard to sneak up on...</string>
  <string name="NED/RandomConvo 15_Intro_001">I wonder if there is a way to attact them into a trap?</string>
  <string name="NED/RandomConvo 15_Intro_002">What do birds eat again?</string>
  <string name="NED/RandomConvo 16_Intro_000">You ever see something strange out here at night?</string>
  <string name="NED/RandomConvo 16_Intro_001">No, I don't mean me.</string>
  <string name="NED/RandomConvo 17_Intro_000">&lt;proud&gt;I never run away from a challenge.</string>
  <string name="NED/RandomConvo 17_Intro_001">Unless that challenge has big teeth and it's way to close.</string>
  <string name="NED/RandomConvo 18_Intro_000">It's so good to see a youngen like yourself out here exploring.</string>
  <string name="NED/RandomConvo 18_Intro_001">We need more people like you, &lt;PlayerName&gt;.</string>
  <string name="NED/RandomConvo 1_Intro_000">&lt;no&gt;There ain't nothing like living off the land.</string>
  <string name="NED/RandomConvo 1_Intro_001">It's bloody thrilling!</string>
  <string name="NED/RandomConvo 2_Intro_000">&lt;no&gt;Weapons aren’t toys you know.</string>
  <string name="NED/RandomConvo 2_Intro_001">&lt;yes&gt;They are tools we use to defend ourselves and hunt for food.</string>
  <string name="NED/RandomConvo 3_Intro_000">Where am I from?</string>
  <string name="NED/RandomConvo 3_Intro_001">I'm from the wilds, mate.</string>
  <string name="NED/RandomConvo 3_Intro_002">Grew up wild and I'll bloody die wild.</string>
  <string name="NED/RandomConvo 4_Intro_000">&lt;PlayerName&gt;, you've got heart,  anybody can see that.</string>
  <string name="NED/RandomConvo 4_Intro_001">&lt;yes&gt;Fine quality in a person.</string>
  <!-- Talking about Animal Type: Shark -->
  <string name="NED/RandomConvo 5_Intro_000">I’m not the strongest swimmer.</string>
  <!-- Talking about Animal Type: Shark -->
  <string name="NED/RandomConvo 5_Intro_001">&lt;worried&gt;I steer clear of &lt;animalName&gt;.</string>
  <!-- Talking about Animal Type: Shark -->
  <string name="NED/RandomConvo 5_Intro_002">&lt;proud&gt;They stay in the water and I stay on the land.</string>
  <!-- Talking about Animal Type: Croco -->
  <string name="NED/RandomConvo 6_Intro_000">&lt;no&gt;Never smile at a &lt;animalName&gt;.</string>
  <!-- Talking about Animal Type: Croco -->
  <string name="NED/RandomConvo 6_Intro_001">&lt;no&gt;And you certainly don't want to see them smile back at you!</string>
  <string name="NED/RandomConvo 7_Intro_000">Hunting is a fine sport, &lt;PlayerName&gt;.</string>
  <string name="NED/RandomConvo 7_Intro_001">&lt;proud&gt;But there is one thing I can't hunt down...</string>
  <string name="NED/RandomConvo 7_Intro_002">&lt;haha&gt;A true blue challenge.</string>
  <string name="NED/RandomConvo 8_Intro_000">If I had any advice for a whipper snapper like yourself it'd be this;</string>
  <string name="NED/RandomConvo 8_Intro_001">Find a passion,  and hold on to it like it owes you something!</string>
  <string name="NED/RandomConvo 8_Intro_002">Go at it tooth and nail! &lt;pumped&gt;</string>
  <string name="NED/RandomConvo 9_Intro_000">&lt;proud&gt;Weapons and me go together like spaghetti and blankets.</string>
  <string name="NED/SomethingToSellTrapper_Intro_000">Very nice indeed.</string>
  <string name="NED/SomethingToSellTrapper_Intro_001">Tell you what, how does &lt;$&gt; sound?</string>
  <string name="NED/SomethingToSellTrapper_Option_000">Deal.</string>
  <string name="NED/SomethingToSellTrapper_Option_001">No Deal.</string>
  <string name="NED/SomethingToSellTrapper_Response_000_000">You beauty!</string>
  <string name="NED/SomethingToSellTrapper_Response_000_001">I'll take these items and here is your &lt;$&gt; .</string>
  <string name="NED/SomethingToSellTrapper_Response_000_002">Pleasure as always, &lt;PlayerName&gt;.</string>
  <string name="NED/SomethingToSellTrapper_Response_001_000">&lt;no&gt;Righto.</string>
  <string name="NED/SomethingToSellTrapper_Response_001_001">&lt;wave&gt;See you around, &lt;PlayerName&gt;.</string>
  <string name="NED/TotalEqualsZeroTrapper_Intro_000">All right... Lets see...</string>
  <string name="NED/TotalEqualsZeroTrapper_Intro_001">It seems these items aren't worth anything, but I can still take them off your hands if you don't want them?</string>
  <string name="NED/TotalEqualsZeroTrapper_Option_000">Yes, I dont want them.</string>
  <string name="NED/TotalEqualsZeroTrapper_Option_001">I'll have them back thanks.</string>
  <string name="NED/TotalEqualsZeroTrapper_Response_000_000">Righto, mate.</string>
  <string name="NED/TotalEqualsZeroTrapper_Response_000_001">Cheers.</string>
  <string name="NED/TotalEqualsZeroTrapper_Response_001_000">That's fine, please return if you wish to sell anything else.</string>
  <string name="NED/TotalEqualsZeroTrapper_Response_001_001">Thank you for visiting John's Goods!</string>
  <string name="NED/Trapper AfterCraftingItem_Intro_000">There you go, mate!</string>
  <string name="NED/Trapper AfterCraftingItem_Intro_001">There is your &lt;itemName&gt;!</string>
  <string name="NED/Trapper AfterCraftingItem_Intro_002">&lt;proud&gt;What do you reckon? Pretty good, eh?</string>
  <string name="NED/Trapper AfterCraftingItem_Intro_003">Righto, is that all you need?</string>
  <string name="NED/Trapper AfterCraftingItem_Option_000">Make something else</string>
  <string name="NED/Trapper AfterCraftingItem_Option_001">All done.</string>
  <string name="NED/Trapper AfterCraftingItem_Response_000_000">Alright then.</string>
  <!-- Talking about NPC: Fletch. Talking about Item: Sky Fest Prize Ticket -->
  <string name="NED/SkyFest_AfterNoon_005_Intro_000">Happy &lt;SkyFest&gt;!</string>
  <string name="NED/SkyFest_Morning_005_Intro_000">Happy &lt;SkyFest&gt;!</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/SkyFest_Night_005_Intro_000">It’s amazing to see the sky like this!</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/SkyFest_Night_005_Intro_001">&lt;yes&gt;I always love a good &lt;SkyFest&gt; celebration.</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/SkyFest_Night_005_Intro_002">&lt;glee&gt;&gt;But I reckon a &lt;SkyFest&gt; on &lt;IslandName&gt; will always be the best kind of &lt;SkyFest&gt;.</string>
  <!-- Talking about NPC: Fletch -->
  <string name="NED/SkyFest_Noon_005_Intro_000">Nice &lt;Kite&gt;, mate!</string>
  <string name="NED/Trapper AfterCraftingItem_Response_001_000">Righto, mate.</string>
  <string name="NED/Ned_Salesman_Intro_000">Ah, &lt;PlayerName&gt;!</string>
  <string name="NED/Ned_Salesman_Intro_001">Fancy bumping into you out here.</string>
  <string name="NED/Ned_Salesman_Intro_002">Do you need something?</string>
  <string name="NED/Ned_Salesman_Option_000">What can you make me?</string>
  <string name="NED/Ned_Salesman_Option_001">I want to chat</string>
  <string name="NED/Ned_Salesman_Option_002">Thanks.</string>
  <string name="NED/Ned_Salesman_Response_000_000">Let's see what we can do for you.</string>
  <string name="NED/Ned_Salesman_Trapper_Response_002_000">&lt;wave&gt;Good hunting, mate.</string>
  <string name="NED/SomethingToSellJimmy_Intro_000">&lt;thinking&gt;Oh, yes... all these bulk items...</string>
  <string name="NED/SomethingToSellJimmy_Intro_001">Plus my very, very fair bonus...</string>
  <string name="NED/SomethingToSellJimmy_Intro_002">I can give you &lt;$&gt;. Take it or leave it.</string>
  <string name="NED/SomethingToSellJimmy_Option_000">Take it.</string>
  <string name="NED/SomethingToSellJimmy_Option_001">Leave it.</string>
  <string name="NED/SomethingToSellJimmy_Response_000_000">Very good. Yes. Very good.</string>
  <string name="NED/SomethingToSellJimmy_Response_000_001">Thank you for your goods here, suck- I mean &lt;PlayerName&gt;.</string>
  <string name="NED/SomethingToSellJimmy_Response_000_002">&lt;yes&gt;Here is your &lt;$&gt;.</string>
  <string name="NED/SomethingToSellJimmy_Response_000_003">&lt;proud&gt;Pleasure as always.</string>
  <string name="NED/SomethingToSellJimmy_Response_001_000">&lt;no&gt;It's a shame really, missing out on a great deal you are.</string>
  <string name="NED/Weapon_Shop_Intro_000">Need some protection? Take a look at my wares.</string>
  <string name="NED/Weapon_Shop_Intro_001">This stuff can protect you from the harsh wild.</string>
  <string name="NED/Weapon_Shop_Option_000">I want to chat</string>
  <string name="NED/Weapon_Shop_Option_001">Thanks.</string>
  <string name="NED/Weapon_Shop_Response_001_000">As you were.</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_0_Line_0">Have you ever tried to make a new plant?</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_0_Line_1">How would I do that?</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_0_Line_2">Perhaps you could splice the seeds?</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_0_Line_3">I could never do that!</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_0_Line_4">I do not understand…</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_0_Line_5">It would hurt the seeds!</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_1_Line_0">I require your assistance.</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_1_Line_1">Sure!</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_1_Line_2">Please stop bothering me when I am trying to work.</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_1_Line_3">I thought I was helping…</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_1_Line_4">You were not.</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_2_Line_0">What is your favourite plant?</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_2_Line_1">Like, it’s hard to pick one type…</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_2_Line_2">Type? I want to see the specific plant.</string>
  <!-- Said by Rayne talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Rayne_2_Line_3">I, like, love them all equally.</string>
  <!-- Said by Ted Selly talking to Rayne -->
  <string name="NED/BackAndForthConvo_Rayne_2_Line_4">Impossible.</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_0_Line_0">Your tools are inferior to anything I can craft.</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_0_Line_1">My tools are much cheaper though.</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_0_Line_2">Yes they are.</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_0_Line_3">But, they get the job done!</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_0_Line_4">Yes they do. Slowly.</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_0_Line_5">Why do I talk to you…</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_1_Line_0">Are you interested in stocking some of my inventions?</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_1_Line_1">That sounds great!</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_1_Line_2">But I need to run my shop!</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_1_Line_3">Then this business proposal is now moot.</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_2_Line_0">Well, I have to send a lot back home…</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_2_Line_1">I suppose someone back in South City is doing research for you?</string>
  <!-- Said by Ted Selly talking to John -->
  <string name="NED/BackAndForthConvo_John_2_Line_2">You could say that…</string>
  <!-- Said by John talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_John_2_Line_3">Then I hope you share what you have learnt soon.</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_0_Line_0">So this power can allow you to see the future?</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_0_Line_1">Sometimes.</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_0_Line_2">And how can I access this power?</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_0_Line_3">You need to feel it, y’know?</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_0_Line_4">Hmmm. Kinetic energy of some sort..</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_0_Line_5">No. Not at all!</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_1_Line_0">What do you mean my coat is drab?</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_1_Line_1">You should come and see me.</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_1_Line_2">I am seeing you now and my coat is just as functional as ever.</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_1_Line_3">I could make you a nicer coat!</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_1_Line_4">No thank you. This one is adequate.</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_2_Line_0">I have an idea for a motorised machine for sewing.</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_2_Line_1">Oh wow!</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_2_Line_2">It will pull in a lot of power though.</string>
  <!-- Said by Clover talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Clover_2_Line_3">Oh, but I don’t want it to mess with my chakras.</string>
  <!-- Said by Ted Selly talking to Clover -->
  <string name="NED/BackAndForthConvo_Clover_2_Line_4">I see.</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_0_Line_0">No such license exists.</string>
  <!-- Said by Fletch talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Fletch_0_Line_1">Well it flamin’ should!</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_0_Line_2">That is not for me to decide.</string>
  <!-- Said by Fletch talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Fletch_0_Line_3">Just… be careful, will you?</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_0_Line_4">Fine. I will sell only four at a time.</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_1_Line_0">Do you remember much about the Shiny Discs?</string>
  <!-- Said by Fletch talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Fletch_1_Line_1">How old do you think I am?!</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_1_Line_2">Honestly, I could not begin to estimate.</string>
  <!-- Said by Fletch talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Fletch_1_Line_3">Well it was before my time, I’ll tell you what!</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_2_Line_0">Did your father ever venture into the deep mines?</string>
  <!-- Said by Fletch talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Fletch_2_Line_1">Oh, my father went everywhere, I’ll tell you what!</string>
  <!-- Said by Ted Selly talking to Fletch -->
  <string name="NED/BackAndForthConvo_Fletch_2_Line_2">Then perhaps you would answer a few questions?</string>
  <!-- Said by Fletch talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Fletch_2_Line_3">Sure, how long you got?</string>
  <!-- Said by Ted Selly talking to Irwin -->
  <string name="NED/BackAndForthConvo_Irwin_0_Line_0">Have you performed any experiments on your creatures?</string>
  <!-- Said by Irwin talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Irwin_0_Line_1">Um… no? What?</string>
  <!-- Said by Ted Selly talking to Irwin -->
  <string name="NED/BackAndForthConvo_Irwin_0_Line_2">You could make them stronger, faster, better!</string>
  <!-- Said by Irwin talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Irwin_0_Line_3">I think they’re fine just the way they are, thanks.</string>
  <!-- Said by Ted Selly talking to Irwin -->
  <string name="NED/BackAndForthConvo_Irwin_1_Line_0">But with the right upgrades...</string>
  <!-- Said by Irwin talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Irwin_1_Line_1">I heard you, I’m just not interested, mate.</string>
  <!-- Said by Ted Selly talking to Irwin -->
  <string name="NED/BackAndForthConvo_Irwin_1_Line_2">Think of the possibilities!</string>
  <!-- Said by Irwin talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Irwin_1_Line_3">Just leave nature alone, yeah?</string>
  <!-- Said by Ted Selly talking to Irwin -->
  <string name="NED/BackAndForthConvo_Irwin_2_Line_0">Does the smell not bother you?</string>
  <!-- Said by Irwin talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Irwin_2_Line_1">A little, but you’re not that bad, mate!</string>
  <!-- Said by Ted Selly talking to Irwin -->
  <string name="NED/BackAndForthConvo_Irwin_2_Line_2">I am speaking of your creatures.</string>
  <!-- Said by Irwin talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Irwin_2_Line_3">Oh, maybe. Some of them have really sensitive noses!</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_0_Line_0">Have you ever considered a different field?</string>
  <!-- Said by Sally talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sally_0_Line_1">Further north, you mean?</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_0_Line_2">I mean some other occupation.</string>
  <!-- Said by Sally talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sally_0_Line_3">Oh. All the time! But what about my customers?</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_0_Line_4">I believe they would survive.</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_1_Line_0">I spent yesterday arranging my instruments.</string>
  <!-- Said by Sally talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sally_1_Line_1">Is that why you cancelled your appointment?</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_1_Line_2">I did not make an appointment.</string>
  <!-- Said by Sally talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sally_1_Line_3">I’m free tomorrow, my client list is pretty short.</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_1_Line_4">Of that, I am certain.</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_2_Line_0">Have you considered adding restraints to your chair?</string>
  <!-- Said by Sally talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sally_2_Line_1">I don’t think it’s going to go anywhere.</string>
  <!-- Said by Ted Selly talking to Sally -->
  <string name="NED/BackAndForthConvo_Sally_2_Line_2">I was thinking of your customers.</string>
  <!-- Said by Sally talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sally_2_Line_3">Oh, might be an idea, then. Safety first!</string>
  <!-- Said by Ted Selly talking to Theodore -->
  <string name="NED/BackAndForthConvo_Theodore_0_Line_0">What will you do when your collection is complete?</string>
  <!-- Said by Theodore talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Theodore_0_Line_1">Categorization and filing!</string>
  <!-- Said by Ted Selly talking to Theodore -->
  <string name="NED/BackAndForthConvo_Theodore_0_Line_2">Of course, but what then?</string>
  <!-- Said by Theodore talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Theodore_0_Line_3">Re-categorization and re-filing!</string>
  <!-- Said by Ted Selly talking to Theodore -->
  <string name="NED/BackAndForthConvo_Theodore_1_Line_0">Do you dissect your specimens?</string>
  <!-- Said by Theodore talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Theodore_1_Line_1">Oh, no! I could never do that!</string>
  <!-- Said by Ted Selly talking to Theodore -->
  <string name="NED/BackAndForthConvo_Theodore_1_Line_2">Then you are missing out on so much knowledge!</string>
  <!-- Said by Theodore talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Theodore_1_Line_3">I think um, maybe that’s okay?</string>
  <!-- Said by Ted Selly talking to Theodore -->
  <string name="NED/BackAndForthConvo_Theodore_2_Line_0">How did you collect all of your specimens?</string>
  <!-- Said by Theodore talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Theodore_2_Line_1">Oh! &lt;PlayerName&gt; has been helping.</string>
  <!-- Said by Ted Selly talking to Theodore -->
  <string name="NED/BackAndForthConvo_Theodore_2_Line_2">What a remarkable coincidence.</string>
  <!-- Said by Theodore talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Theodore_2_Line_3">Yeah, they’ve been really helpful!</string>
  <!-- Said by Ted Selly talking to Milburn -->
  <string name="NED/BackAndForthConvo_Milburn_0_Line_0">If all your Dinks could buy infinite knowledge, would you part with them?</string>
  <!-- Said by Milburn talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Milburn_0_Line_1">Hmph. Certainly not.</string>
  <!-- Said by Ted Selly talking to Milburn -->
  <string name="NED/BackAndForthConvo_Milburn_0_Line_2">But you would know everything.</string>
  <!-- Said by Milburn talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Milburn_0_Line_3">If I need something done, I would simply pay someone.</string>
  <!-- Said by Ted Selly talking to Milburn -->
  <string name="NED/BackAndForthConvo_Milburn_1_Line_0">&lt;IslandName&gt; is amazing for research.</string>
  <!-- Said by Milburn talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Milburn_1_Line_1">I wouldn’t call a place crawling with critters amazing.</string>
  <!-- Said by Ted Selly talking to Milburn -->
  <string name="NED/BackAndForthConvo_Milburn_1_Line_2">But you can play with the very laws of nature here!</string>
  <!-- Said by Milburn talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Milburn_1_Line_3">If there were any laws I would have started suing long ago.</string>
  <!-- Said by Ted Selly talking to Milburn -->
  <string name="NED/BackAndForthConvo_Milburn_2_Line_0">I am developing a machine that can turn tin into gold</string>
  <!-- Said by Milburn talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Milburn_2_Line_1">If you need someone to fund your research, talk to me.</string>
  <!-- Said by Ted Selly talking to Milburn -->
  <string name="NED/BackAndForthConvo_Milburn_2_Line_2">Now that you mention it, I do require a large sum of &lt;Dinks&gt;.</string>
  <!-- Said by Milburn talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Milburn_2_Line_3">Large sum? Count me out.</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_0_Line_0">How do you sterilise your equipment?</string>
  <!-- Said by Sheila talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sheila_0_Line_1">Sterra what?</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_0_Line_2">I mean, how do you clean everything?</string>
  <!-- Said by Sheila talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sheila_0_Line_3">Just good old fashioned elbow grease, darl</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_0_Line_4">…I feel ill.</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_1_Line_0">They are necessary for my work.</string>
  <!-- Said by Sheila talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sheila_1_Line_1">I just never see you wear them.</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_1_Line_2">You never see me at work.</string>
  <!-- Said by Sheila talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sheila_1_Line_3">You sure they’re just not for show?</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_2_Line_0">Do you know why the food has these effects?</string>
  <!-- Said by Sheila talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sheila_2_Line_1">What difference does that make?</string>
  <!-- Said by Ted Selly talking to Sheila -->
  <string name="NED/BackAndForthConvo_Sheila_2_Line_2">To achieve a deeper understanding!</string>
  <!-- Said by Sheila talking to Ted Selly -->
  <string name="NED/BackAndForthConvo_Sheila_2_Line_3">Would that make it taste better?</string>
