  <string name="NED/SkyFest_AfterNoon_005_Intro_000">Happy &lt;SkyFest&gt;!</string>
  <string name="NED/SkyFest_Morning_005_Intro_000">Happy &lt;SkyFest&gt;!</string>
  <!-- Talking about NPC: Fletch -->
