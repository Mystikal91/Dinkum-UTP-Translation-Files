    <string name="MELVIN/RandomConvo_000_Response_000_000">&lt;yes&gt;Non c\'è niente di male in questo.</string>
    <string name="MELVIN/RandomConvo_000_Response_001_000">&lt;no&gt;Non posso dire lo stesso.</string>
    <string name="MELVIN/RandomConvo_001_Intro_001">Forse dovresti provarci, eh, &lt;PlayerName&gt;?</string>
    <string name="MELVIN/RandomConvo_001_Intro_000">&lt;yes&gt;Oggi ho intenzione di prendermi il mio tempo.</string>
    <string name="MELVIN/RandomConvo_002_Option_000">Bene, grazie!</string>
    <string name="MELVIN/RandomConvo_002_Intro_000">Come va, &lt;PlayerName&gt;?</string>
    <string name="MELVIN/RandomConvo_002_Option_001">Ho avuto giorni migliori</string>
    <string name="MELVIN/RandomConvo_002_Response_000_000">&lt;yes&gt;È un\'ottima notizia.</string>
    <string name="MELVIN/RandomConvo_002_Response_001_000">Mi dispiace.</string>
    <string name="MELVIN/RandomConvo_002_Response_001_001">Spero che la situazione migliori.</string>
    <string name="MELVIN/RandomConvo_003_Intro_000">&lt;yes&gt;Mi piace costruire cose.</string>
    <string name="MELVIN/RandomConvo_003_Intro_001">&lt;shy&gt;Tuttavia, cerco di prendermi il tempo necessario.</string>
    <string name="MELVIN/RandomConvo_004_Intro_000">&lt;thinking&gt;Gli alberi qui su &lt;IslandName&gt; sembrano avere un buon legno per costruire cose.</string>
    <string name="MELVIN/RandomConvo_005_Intro_000">&lt;no&gt;Credo che non si sia mai troppo vecchi per fare qualcosa.</string>
    <string name="MELVIN/RandomConvo_006_Intro_000">Sempre con qualcosa da fare, vero &lt;PlayerName&gt;?</string>
    <string name="MELVIN/RandomConvo_006_Intro_001">&lt;no&gt;Se corressi come te, credo che mi cadrebbero le ginocchia.</string>
    <string name="MELVIN/RandomConvo_007_Intro_000">Non lavorare troppo, &lt;PlayerName&gt;?</string>
    <string name="MELVIN/RandomConvo_007_Intro_001">&lt;no&gt;Non c\'è motivo di affrettarsi.</string>
    <string name="MELVIN/RandomConvo_007_Intro_002">&lt;yes&gt;C\'è sempre domani.</string>
    <string name="MELVIN/RandomConvo_008_Intro_002">&lt;thinking&gt;Domani potrebbe piovere, credo.</string>
