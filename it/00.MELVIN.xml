    <string name="MELVIN/BackAndForthConvo_Franklyn_0_Line_2">Mi piacerebbe averne due belle fresche, credo.</string>
    <string name="MELVIN/BackAndForthConvo_Irwin_2_Line_0">Sei un bravo ragazzo.</string>
    <string name="MELVIN/BackAndForthConvo_Rayne_0_Line_0">Tutto bene?</string>
