    <string name="MELVIN/BackAndForthConvo_dayFallbacks_0_Line_1">Sì, lo è.</string>
    <string name="MELVIN/BackAndForthConvo_dayFallbacks_1_Line_0">Come stai?</string>
    <string name="MELVIN/BackAndForthConvo_dayFallbacks_1_Line_2">Bene.</string>
    <string name="MELVIN/BackAndForthConvo_dayFallbacks_2_Line_1">Lo dici sempre.</string>
    <string name="MELVIN/BackAndForthConvo_nightFallbacks_0_Line_0">\'Sera</string>
    <string name="MELVIN/BackAndForthConvo_nightFallbacks_0_Line_1">Ehilà!</string>
    <string name="MELVIN/BackAndForthConvo_nightFallbacks_1_Line_1">Buonanotte!</string>
    <string name="MELVIN/BackAndForthConvo_nightFallbacks_2_Line_0">Bella serata.</string>
    <string name="MELVIN/BackAndForthConvo_nightFallbacks_2_Line_1">Sì, è veramente bella.</string>
    <string name="MELVIN/BackAndForthConvo_nightFallbacks_2_Line_2">Davvero bella.</string>
    <string name="MELVIN/BackAndForthConvo_rainingFallbacks_0_Line_0">Meglio la pioggia che la neve.</string>
    <string name="MELVIN/BackAndForthConvo_rainingFallbacks_0_Line_1">Troppo vero.</string>
    <string name="MELVIN/BackAndForthConvo_rainingFallbacks_1_Line_0">Piove di nuovo.</string>
    <string name="MELVIN/BackAndForthConvo_rainingFallbacks_1_Line_1">Sì, lo vedo.</string>
    <string name="MELVIN/BackAndForthConvo_rainingFallbacks_2_Line_1">Perché lo dici?</string>
