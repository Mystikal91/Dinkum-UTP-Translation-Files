    <string name="MELVIN/Noon_Greeting 0_Intro_000">&lt;yes&gt;Credo che ci sia ancora molto tempo della giornata a disposizione.</string>
    <string name="MELVIN/Noon_Greeting 0_Response_003_000">Non c\'è fretta, &lt;PlayerName&gt;.</string>
